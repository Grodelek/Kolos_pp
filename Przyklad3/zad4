#include <stdio.h>
#include <stdlib.h>

int main()
{
    double x[4];
    double y[4];
    double a; double b;
    int rozmiarY=0;
    printf("Podaj dolny przedzial :\n");
    scanf("%lf",&a);
    printf("Podaj gorny przedzial: \n");
    scanf("%lf",&b);
    printf("Podaj 4 liczby tablicy x: \n");
    for(int i=0; i<4;i++){
        scanf("%lf",&x[i]);
    }
    for(int i=0;i<4;i++){
        if(x[i] >= a && x[i] <= b){
             y[rozmiarY] = x[i];
             rozmiarY++;
        }
    }    double max=0;

    for(int i=0;i<rozmiarY;i++){
        printf("%lf ",y[i]);
        if(y[i]>max){
            max = y[i];
        }
    }
    printf("\n %lf to maksymalna liczba\n",max);

   return 0;
}
